package com.springverse.SpringDemo.classes;

import java.time.LocalDate;

public class Marvel {

	private int phase;
	private String project;
	private LocalDate date;
	
	public Marvel(int i,String project,LocalDate date) {
		this.phase = i;
		this.project = project;
		this.date = date;
	}

	public int getPhase() {
		return phase;
	}

	public String getProject() {
		return project;
	}
	
	public LocalDate getDate() {
		return date;
	}
	
	@Override
	public String toString() {
		return "Phase = "+phase+"\n"
				+"Project = "+project;
	}
	
}
