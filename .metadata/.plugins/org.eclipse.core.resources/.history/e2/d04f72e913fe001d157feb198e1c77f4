package com.springverse.SpringDemo.classes;

public class Marvel {

	private int phase;
	private String project;
	
	public Marvel(int i,String project) {
		this.phase = i;
		this.project = project;
	}

	public int getPhase() {
		return phase;
	}

	public String getProject() {
		return project;
	}

	
	
	@Override
	public String toString() {
		return "Phase = "+phase+"\n"
				+"Project = "+project;
	}
	
}
