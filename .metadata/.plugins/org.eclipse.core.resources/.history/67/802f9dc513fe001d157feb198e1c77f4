package com.springverse.SpringDemo.classes;

import java.time.LocalDate;

public class Marvel {

	private byte phase;
	private String project;
	private LocalDate date;
	
	public Marvel(byte phase,String project,LocalDate date) {
		this.date = date;
		this.project = project;
		this.date = date;
	}

	public byte getPhase() {
		return phase;
	}

	public String getProject() {
		return project;
	}

	public LocalDate getDate() {
		return date;
	}
	
	@Override
	public String toString() {
		return "Phase = "+phase+"\n"
				+"Project = "+project+"\n"
				+"Date = "+date;
	}
	
}
