package com.springverse.SpringDemo.classes;

import java.time.LocalDate;
import java.time.Month;

public class Marvel {

	private int phase;
	private String project;
	private Month month;
	
	public Marvel(int i,String project,Month month) {
		this.phase = i;
		this.project = project;
		this.month = month;
	}

	public int getPhase() {
		return phase;
	}

	public String getProject() {
		return project;
	}
	
	public Month getMonth() {
		return month;
	}
	
	@Override
	public String toString() {
		return "Phase = "+phase+"\n"
				+"Project = "+project+"\n"
				+"Month = "+month;
	}
	
}
